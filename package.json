{
  "name": "@desvart/bitburner-v2",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "Bitburner",
    "BDD",
    "hexagonal architecture",
    "ATDD",
    "cucumber",
    "testing",
    "TypeScript"
  ],
  "homepage": "https://github.com/Desvart/bitburner-v2#readme",
  "repository": "https://github.com/Desvart/bitburner-v2.git",
  "bugs": {
    "url": "https://github.com/Desvart/bitburner-v2/issues",
    "email": "derek.svart@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "author": "Desvart <derek.svart@gmail.com>",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint ./**/*.ts",
    "lintTest": "eslint ./**/*.ts",
    "fix": "eslint ./**/*.ts --fix",
    "pretest": "",
    "unit": "jest --coverage .*.test.ts",
    "cucumber_old": "SET NODE_OPTIONS=--loader ts-node/esm --no-warnings --experimental-specifier-resolution=node --enable-source-maps && cucumber-js --config=\"./config/cucumber.json\"",
    "cucumber": "node --loader ts-paths-esm-loader --no-warnings --experimental-specifier-resolution=node --enable-source-maps node_modules/@cucumber/cucumber/bin/cucumber.js",
    "test": "npm run unit && npm run cucumber",
    "test:mutation": "stryker run",
    "posttest": "",
    "precompile": "npm run test",
    "clean": "mkdir dist && rmdir /s /q dist && mkdir dist",
    "build": "tsc --build --clean && tsc",
    "compile": "npm run build",
    "deploy": "bitburner-sync",
    "debug": "npm run build && npm run deploy"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.1.0",
    "@stryker-mutator/core": "^6.4.2",
    "@stryker-mutator/jest-runner": "^6.4.2",
    "@stryker-mutator/typescript-checker": "^6.4.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "bitburner-sync": "^1.5.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "jest-html-reporters": "^3.1.4",
    "jest-slow-test-reporter": "^1.0.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.4.1",
    "typescript": "^5.0.3"
  }
}
